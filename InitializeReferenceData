        void createSignalAssetObject(string APS, string cab1type, string detect, boolean evp, string evpdir,  
         							 string shopNo, string signalGeo, string vidDecType){
                                         
             SignalAsset__c assetEntry = new SignalAsset__c();
             
             assetEntry.APS__c = APS;	//Multipicklist
             assetEntry.Cab1Type__c = cab1type; 		//Picklist
             assetEntry.CCTV__c = false;		//Boolean, every entry is false
             assetEntry.Detection__c = detect;			//Multipicklist           
             assetEntry.EVP__c = evp;	//Checkbox (Boolean)
             assetEntry.EVPDirection__c = evpdir; //Multipicklist
             assetEntry.Cab2HubCabinet__c = false;	//Checkbox (Boolean), every entry is false
             assetEntry.RailroadPreempt__c = false;	//Checkbox (Boolean), every entry is false
             
             ShopNumber__c sh = [select IntersectionName1__c from ShopNumber__c where Name = :shopNo Limit 1]; 
             assetEntry.ShopNumber__c = sh.Id;		//Master-Detail(Shop Number)
             
             assetEntry.SignalGeofilter__c = signalGeo;		//Multipick 
             assetEntry.TSP__c = false;			//Checkbox (Boolean) every entry is false
             assetEntry.VideoDetectionType__c = vidDecType;  //Picklist
             assetEntry.WirelessRadio__c = '';			//Multipick
             
             insertSignalAsset(assetEntry);
             
             //***************UNWRITTEN FIELDS**********************
             //assetEntry.Cab2Type__c = cab2type; 		//Picklist, every entry is blank
             //assetEntry.CCTVType__c = cctvType;			//Multipicklist, every entry is blank
             //assetEntry.Cab1ControllerType__c = cab1Cont;	//Picklist, every entry is blank
             //assetEntry.Cab2ControllerType__c = cab2Cont;	//Picklist, every entry is blank
             //assetEntry.Cab1Encoder__c = cab1En;		//Multipicklist
             //assetEntry.Cab2Encoder__c = cab2En; 	//Multipicklist   
             //assetEntry.Cab1NetworkSwitches__c = network1;	//Multipick, every entry is blank
             //assetEntry.Cab2NetworkSwitches__c = network2; //Multipick, every entry is blank
             //assetEntry.DetectorIOModule__c = detIOMod;	//Picklist
             //assetEntry.ShopNumberSortable__c = shopNoSort;	//Formula(Number) unwriteable
             //assetEntry.ShopStatus__c = shopStatus;			//Formula(Text) unwriteable
             //assetEntry.ShopType__c = shopType;				//Formula(Text) unwriteable
             //assetEntry.Name = '';				//String unwriteable
             //assetEntry.Cab1SoftwareUsed__c = cab1Soft;		//Picklist blank entries
             //assetEntry.Cab2SoftwareUsed__c = cab2Soft;		//Picklist blank entries
             //assetEntry.IntersectionLocation__c = intLoc;//Formula(Text) blank entries
             
             //assetEntry.VideoDetectionDetail__c = vidDecDet;	//Multipick
         }
        
        void insertSignalAsset(SignalAsset__c signalAssetEntryIn){
            assetList.add(signalAssetEntryIn);
        }
        
        void insertAssetList(){
            insert assetList;
        }
        
    }	//end class SignalAssets ((done))
    
    public class SignalOperation{
        
        List<SignalOperation__c> operationList = new List<SignalOperation__c>();
        
        
        public void submitSignalOperationData(){    
            
            createSignalOperationObject('No Driveway', 'No Driveway', 'Permissive', '', 'Permissive', 'Permissive',
                                        false, false, false, 'Permissive', '01', false);
            
            createSignalOperationObject('No Driveway', 'No Driveway', 'Protected', '', 'Protected', 'Protected',
                                        false, false, false, 'Protected', '02', false);
            
            createSignalOperationObject('No Driveway', 'No Driveway', 'Permissive', '', 'Permissive', 'Permissive',
                                        false, false, false, 'Permissive', '03', false);
            
            createSignalOperationObject('Full', 'Commercial', '', '', 'Permissive', 'Permissive',
                                        false, false, true, 'Permissive', '04', false);
            
            createSignalOperationObject('No Driveway', 'No Driveway', 'Permissive', '', 'Permissive', 'Permissive',
                                        false, false, false, 'Permissive', '05', false);
            
            createSignalOperationObject('No Driveway', 'No Driveway', '', 'Protected', 'Protected', 'Protected',
                                        false, false, false, 'Protected', '06', false);
            
            createSignalOperationObject('No Driveway', 'No Driveway', '', 'Protected', 'Permissive', 'Permissive',
                                        false, false, false, 'Permissive', '07', false);
            
