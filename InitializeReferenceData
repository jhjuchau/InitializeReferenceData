        public void createIntersectionObject(
            string aDirection, string aName, string aType, 
            string bDirection, string bName, string bType, decimal latitude, decimal longitude,
            string intersectionType, string trafficControlType){ 
                
                Intersection__c entry = new Intersection__c();
                
                //entry.ID = 'fdsafdasd';
                
                entry.AStreetDirection__c = aDirection;	//Picklist values: --None--, North-South, East-West
                entry.AStreetName__c = aName;							
                entry.AStreetType__c = aType;			//Picklist values: --None--, Freeway/Expressway (CA), Local (LO),  
                //				Major Street (MA), Neighborhood Collector (NC)	
                entry.Active__c = true;
                entry.BStreetDirection__c = bDirection;	//Picklist values: --None--, North-South, East-West
                entry.BStreetName__c = bName;
                entry.BStreetType__c = bType;			//Picklist values: --None--, Freeway/Expressway (CA), Local (LO),  
                //				Major Street (MA), Neighborhood Collector (NC)	
                
                entry.geolocation__latitude__s = latitude;
                entry.geolocation__longitude__s = longitude;
                
                entry.IntersectionType__c = intersectionType;	//Picklist values: --None--, 2 Leg, 3 Leg, 4 Leg, Light Rail Crossing
                entry.QStreetNameA__c = ' ';	//this field is blank on every entry 
                entry.QStreetNameB__c = ' '; 	//this field is blank on every entry
                entry.TrafficControlType__c = trafficControlType; //Picklist values: --None--, 1 Way Stop, 2 Way Stop, 3 Way Stop, 4 Way Stop, 
                //					No Control, Signal
                //Use No Control instead of --None-- for clarity
                
                appendToList(entry);
                
            }	//end method CreateReferenceIntersection
        
        public void appendToList(Intersection__c newEntry){	//Inserts intersections into the database
            intersectionList.add(newEntry);
        }	//end method insertIntersections
        
        public void insertIntersections(){
            insert intersectionList;
        }
        
    } //end class Intersections ((done))
    
