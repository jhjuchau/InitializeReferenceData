        public void createRadarObject(string bulbType, string direction, string distance, string existing,
         							  decimal lat, decimal lng, string loc, string mode, string school,
        							  string newPole, string notes,  string poleID, string spLimit,
          							  string proposed, string name, string shopNo){
               RadarSign__c rEntry = new RadarSign__c();
               //[SELECT ShopNumber__c FROM RadarSign__c WHERE RadarSign__c.ShopNumber__c =: shopNo LIMIT 1];
               
               
               rEntry.BulbType__c = bulbType;				//Picklist: Conventional, LED, Planned LED, Proposed LED
               rEntry.Direction__c = direction;			//Picklist: --None--, Northbound, Southbound, Westbound, Eastbound
               rEntry.DistanceToSchool__c = distance;		//0-1000 FT (ie '590 FT')
               rEntry.ExistingSignType__c = existing;		//Picklist: --None--, AC, AC*, Solar, PG&E*, New
               rEntry.Geolocation__Latitude__s = lat;	
               rEntry.Geolocation__Longitude__s = lng;
               rEntry.Location__c = loc;				//Address (ie "50' N/O Upton Wy" or "250' W/O Taft Drive")
               rEntry.Mode__c = mode;					//Picklist: --None--, A, B, C, D
               rEntry.NearbySchool__c = school;			//Picklist: Gigantic, go see for yourself
               rEntry.NewPoleID__c = newPole;				//Text(10)
               rEntry.Notes__c = notes;					//Text		
               
               rEntry.PoleID__c = poleID;
               rEntry.PostedSpeedLimit__c = spLimit;
               rEntry.ProposedSignType__c = proposed;		//Picklist:
               rEntry.Name = name;
               
               ShopNumber__c sh = [select IntersectionName1__c from ShopNumber__c where Name = :shopNo Limit 1]; 
               rEntry.ShopNumber__c = sh.Id;
               rEntry.Status__c = 'Existing';			//Picklist, but all entries are 'Existing' 
               rEntry.Version__c = 'Speed Check';
               
                                          
               //rEntry.OwnerId = owner;
               appendRadarToList(rEntry);
        }
        
        public void appendRadarToList(RadarSign__c radarEntryIn){
            radarList.add(radarEntryIn);           
        }
        
        public void insertRadarList(){
            insert radarList;
        }
        
    }	//end class RadarSigns ((need proper shop numbers))
    
