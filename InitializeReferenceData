        void createInvObject(string equipModel, string equipType, string equipManu, string OEMno, string serialID)
        {
            IMInventory__c ientry = new IMInventory__c();
            
            ientry.EquipmentModelSearch__c = equipModel;
            ientry.EquipmentModel__c = equipModel;
            
            ientry.EquipmentType__c = equipType;
            ientry.EquipmentTypeSearch__c = equipType;
            
            ientry.EquipmentTypeManufacturer__c = equipManu;
            ientry.EquipmentTypeManufacturerSearch__c = equipManu;
            ientry.OEMSerialNumber__c = OEMno;
            
            ientry.Name = serialID;
            
            appendInvObjectToList(ientry);
            
            
            //*********UNUSED FIELDS************
            //ientry.InstalledShopNumber__c = shopNo;
            //ientry.IntersectionName1__c = interName;
            //ientry.OwnerId = ''; 
            //ientry.PurchaseDate__c = purchaseDate;
            //ientry.EquipmentStatus__c = '';
            //ientry.EquipmentStatusSearch__c = '';
            //ientry.Zone__c = zone        
            
        }
        
        void appendInvObjectToList(IMInventory__c iEntryIn){
            invList.add(iEntryIn);
        }
        
        void insertInvList(){
            insert invList;
        }
    }	//end class IMInventory
    
    public class BaseException extends Exception{}
    
    public static void main()
    {
        
        if ([select IntersectionName1__c from ShopNumber__c Limit 1].isEmpty()){ 
                 
        Intersections intObj = new Intersections();
        ShopNumbers shopObj = new ShopNumbers();
        RadarSigns radarObj = new RadarSigns();
        SignalAssets assetObj = new SignalAssets();
        SignalOperation opObj = new SignalOperation();
        IMInventory invObj = new IMInventory();
       
            
        intObj.submitIntersectionData();
        shopObj.submitShopNumberData();
        radarObj.submitRadarData();
        assetObj.submitSignalAssetData();
        opObj.submitSignalOperationData();
        invObj.submitInvData();
            
        }
        else{
			throw new BaseException('*** ERROR *** In order to run this, your sandbox must be empty. Do NOT run this on Production!');
        }   
        
    }
    
    
} //end class InitializeReferenceData
