        void createSignalOperationObject(string sigInd, string driveways, string nb, string sb, string eb, string wb, 
                                         boolean overlap, boolean LPI, boolean school, string phasing, string shopNo,
                                         boolean seniorTiming){
                    SignalOperation__c sopEntry = new SignalOperation__c();
                                             
                                             
                    sopEntry.DrivewaySignalIndication__c = sigInd;
                    sopEntry.Driveways__c = driveways;
                                             
                    sopEntry.NorthboundPhasing__c = nb;
                    sopEntry.EastboundPhasing__c = eb;
                    sopEntry.SouthboundPhasing__c = sb;
                    sopEntry.WestboundPhasing__c = wb; 
                    sopEntry.OverlapPhasing__c = overlap;
                    sopEntry.SeniorTiming__c = false;
                                             
                    sopEntry.LPI__c = LPI;
                    sopEntry.Phasing__c = phasing; //picklist
                    sopEntry.RailroadPreemption__c = false;	//all entries are false
                    sopEntry.SchoolTiming__c = school;
                                             
                    ShopNumber__c sh = [select IntersectionName1__c from ShopNumber__c where Name = :shopNo Limit 1]; 
                    sopEntry.ShopNumber__c = sh.Id;		//Master-Detail(Shop Number)
                                             
                    appendOperationToList(sopEntry);
                                             
                                             
                    //sopEntry.Scramble__c = false;                                
                    //sopEntry.CoordinatedCorridor__c = '';	//picklist, every entry is blank
                    //sopEntry.Coordination__c = '';			//multipick, every entry is blank
                    //sopEntry.CoversheetURL__c = '';			//URL(255), every entry is blank
                    //sopEntry.Flushplans__c = '';		//picklist, all blank
                    //sopEntry.TransitSignalPreemption__c = transitPre;                                  
                  }
        
        void appendOperationToList(SignalOperation__c signalOperationEntry){
            operationList.add(signalOperationEntry);
            
        }
        
        void insertOperationList(){
            insert operationList;
        }
    } //end class SignalOperation
    
    public class IMInventory{
        
        List <IMInventory__c> invList = new List<IMInventory__c>();
        
