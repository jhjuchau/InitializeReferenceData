        public void createShopNumberObject(
            decimal lat, decimal lng, string i1name, string i2name, string i1num, string i2num,
            string operator, Date activated, string priOwner, decimal priOwnerShare,
            string status, string tempShopNo, string shopType, string zone,
            string secOwner, decimal secOwnerShare,string terOwner, decimal terOwnerShare,
            string name){
                
                ShopNumber__c shopEntry = new ShopNumber__c();
                //All ownership shares are decimals. Original Activate Date is a Date object
                
                
                shopEntry.Geolocation__Latitude__s =  lat;
                shopEntry.Geolocation__Longitude__s = lng;
                shopEntry.IntersectionName1__c = i1name;
                shopEntry.IntersectionName2__c = i2name;
                shopEntry.Operator__c = operator;
                shopEntry.OriginalActivationDate__c = activated; //Date.newInstance(year, month, day);
                shopEntry.PrimaryOwnershipName__c = priOwner;
                shopEntry.PrimaryOwnershipShare__c = priOwnerShare;
                shopEntry.RecordDrawingURL__c = '';
                shopEntry.SecondaryOwnershipName__c = secOwner;	
                shopEntry.SecondaryOwnershipShare__c = secOwnerShare;	    
                shopEntry.Status__c = status; //Picklist: --None--, Existing, Existing-To be modified, Future, Deactivated,
                shopEntry.TempShopNumber__c = tempShopNo;	//some number 0 - 9999
                shopEntry.TertiaryOwnershipName__c = terOwner;
                shopEntry.TertiaryOwnershipShare__c = terOwnerShare;
                
                shopEntry.Type__c = shopType;	//Picklist values: --None--, Traffic Signal, Traffic Signal (Ped Crossing),
               
                shopEntry.Zone__c = zone;		//Picklist values: --None, 1, 2, 3, 4, Deactivated, Cancelled
                
                
                // shopEntry.CreatedById = ' ';
                // shopEntry.CreatedByUsername__c = ' ';
                // shopEntry.IntersectionNumber1__c = ' ';
                // shopEntry.IntersectionNumber2__c = ' ';
                // shopEntry.ShopNumberSortable__c = ' ';	//unwriteable
                // shopEntry.TotalWorkOnLocation__c = ' ';	//unwriteable
                // Cancelled, Owned By Other
                shopEntry.Name = name;
                
                appendShopNumberList(shopEntry);                                 
            }
        
        public void appendShopNumberList(ShopNumber__c shopEntryIn){
            shopNumberList.add(shopEntryIn);
        }
        
        public void insertShopNumberList(){
            insert shopNumberList;
        }
        
    }	//end class ShopNumbers ((done))
    
    public class RadarSigns{	//needs a unique Name
        
        List<RadarSign__c> radarList = new List<RadarSign__c>();
        
        public void submitRadarData(){
            
            createRadarObject('Proposed LED', 'Westbound', '385 FT', 'Solar', 
                              37.37096731, -121.8324969, '125\' N/O Bigwood Ave', 'B', 'Elementary - Darling',
                              '10N18', '', '', '35 mph',
                              'AC', '8101', '8101');  

            
            createRadarObject('LED', 'Westbound', '160 FT', 'AC', 
							  37.34950029, -121.7990931, '5\' E/O Ruboin Dr', 'D', 'Middle - Boeger',
							  '', '', '10R87', '35 mph',
							  'AC', '8103', '8103');  
            
            createRadarObject('Proposed LED', 'Northbound', '650 FT', 'AC*',//existing 
                              37.0, -121.0, '165\' S/O Holly Hill Dr', 'A', 'Middle - Fair',//school
                              '', '', '', '35 mph',//newPole, notes, poleID
                              'AC', '8107', '8107');  //proposed, name, shopNo
            
            createRadarObject('Conventional', 'Westbound', '100 FT', 'AC*',//existing 
                              37.0, -121.0, '30\' W/O Adrian Way', 'D', 'Elementary - Meyer',//school
                              '', '', '11P251', '35 mph',//newPole, notes, poleID
                              'AC', '8109', '8109');  //proposed, name, shopNo
            
